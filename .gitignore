# Laravel специфические файлы
/vendor           # Папка с библиотеками Composer
/node_modules     # Папка с библиотеками NPM
/public/storage   # Символическая ссылка для хранения пользовательских файлов
/storage/*.key    # Ключи, используемые для шифрования, нельзя загружать
/.env             # Файл конфигурации окружения, содержит чувствительные данные
/.env.*.backup    # Резервные копии файлов окружения

# Игнорирование файлов журналов и баз данных
storage/*.sqlite  # SQLite база данных, если она используется
/storage/logs/*.log  # Журналы приложения
/storage/framework/cache/*   # Кэш приложения
/storage/framework/sessions/*  # Сессии пользователей
/storage/framework/views/*.php  # Скомпилированные представления

# Игнорирование зависимостей Composer
/vendor  # Папка с библиотеками Composer (повторное указание на случай, если забудется)

# Игнорирование зависимостей NPM
/node_modules  # Папка с библиотеками NPM

# Игнорирование кэшей
/.phpunit.result.cache  # Кэш тестов PHPUnit
.phpunit.result.cache   # Кэш тестов PHPUnit

# Laravel Telescope (если используется)
/storage/telescope   # Данные Telescope (если используется)
/storage/clockwork   # Данные Clockwork (если используется)

# Vite (если используется для сборки фронтенда)
/public/build  # Скомпилированные ассеты Vite
/.vite  # Папка с временными файлами Vite
npm-debug.log*  # Логи ошибок NPM
yarn-debug.log*  # Логи ошибок Yarn
yarn-error.log*  # Логи ошибок Yarn

# Файлы конфигурации IDE (например, PHPStorm, VSCode)
/.idea/  # Конфигурация PHPStorm
.vscode/  # Конфигурация VSCode
*.swp  # Временные файлы редакторов (например, Vim)
*.swo  # Временные файлы редакторов (например, Vim)
*~  # Временные файлы, создаваемые при редактировании

# Игнорирование файлов деплоя (если используются Homestead или другие инструменты)
/Homestead.yaml  # Конфигурация Homestead
/Homestead.json  # Конфигурация Homestead
.env.example.backup  # Резервные копии файлов окружения
.env.backup  # Резервные копии файлов окружения
.env.testing  # Файл окружения для тестов
.phpstorm.meta.php  # Метаданные PHPStorm
.DS_Store  # Системные файлы macOS
Thumbs.db  # Системные файлы Windows

# Игнорирование отчетов о покрытии тестами
/coverage  # Отчеты о покрытии тестами

# Игнорирование скомпилированных классов
/bootstrap/cache/*.php  # Скомпилированные классы в Bootstrap

# Игнорирование файла composer.lock (если не нужно фиксировать конкретные версии пакетов)
# /composer.lock  # Этот файл фиксирует версии пакетов, его можно исключить, если нужно больше гибкости